{
    "name": "survos/tree-bundle",
    "description": "incorporate jstre, using twig and stimulus and api-platform",
    "keywords": ["symfony-ux", "symfony6"],
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Tac Tacelosky",
            "email": "tacman@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "api-platform/core": "^2.7 || ^3.0",
        "doctrine/doctrine-bundle": "^2.7",
        "doctrine/orm": "^2.12",
        "doctrine/persistence": "^2.0 || ^3.0",
        "survos/core-bundle": "^1.2.6",
        "survos/inspection-bundle": "^1.2.6",
        "symfony/config": "^6.1.0",
        "symfony/dependency-injection": "^6.1.0",
        "symfony/http-kernel": "^6.1.0",
        "symfony/options-resolver": "^6.1.0",
        "symfony/ux-twig-component": "^2.3",
        "tacman/twig-tree-tag": "^v3.0.0",
        "twig/twig": "^3.4"
    },
    "autoload": {
        "psr-4": {
            "Survos\\Tree\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.3-dev"
        },
        "symfony": {
            "require": "^6.1.0"
        }
    },
    "scripts": {
        "phpstan": "vendor/bin/phpstan analyse"
    },
    "require-dev": {
        "phpstan/phpstan": "^1.8",
        "symfony/browser-kit": "^6.1.0",
        "symfony/framework-bundle": "^6.1.0",
        "symfony/phpunit-bridge": "^6.1.0",
        "symfony/twig-bundle": "^6.1.0",
        "symfony/var-dumper": "^6.1.0",
        "symfony/webpack-encore-bundle": "^1.11"
    },
    "conflict": {
        "survos/barcode-bundle": "<1.2.6",
        "survos/second-package": "<1.2.6",
        "survos/faker-bundle": "<1.2.6",
        "survos/first-package": "<1.2.6",
        "survos/maker-bundle": "<1.2.6",
        "survos/html-prettify-bundle": "<1.2.6",
        "survos/api-grid-bundle": "<1.2.6",
        "survos/bootstrap-bundle": "<1.2.6",
        "survos/doc-bundle": "<1.2.6",
        "survos/grid-bundle": "<1.2.6",
        "survos/auth-bundle": "<1.2.6",
        "survos/workflow-helper-bundle": "<1.2.6",
        "survos/crawler-bundle": "<1.2.6"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
